package aa;

public class Car {
// 클래스변수... 속성... property
	private String name;
	private double height;
	private double in_area;
	
	
	
	public Car() {
		System.out.println("기본생성자");
	}
	public void doPrint() {
		System.out.println("이름 = "+name);
		System.out.println("키 = "+height);
		System.out.println("안측넓이 ="+in_area);
	}
}
