import com.java.org.AA;

public class Ex04 {

	public static void main(String[] args) {
	
	AA a1 = new AA();
	System.out.println("a1.a = "+a1.a);
	System.out.println("a1.AMAX = "+a1.AMAX);
	
	a1.addE();
	System.out.println("a1.a = "+a1.a);
	System.out.println("a1.AMAX = "+a1.AMAX);
	
	AA a2 = new AA();
	System.out.println("a2.a = "+a2.a);
	System.out.println("a2.AMAX = "+a2.AMAX);
	
	a2.addE();
	System.out.println("a2.a = "+a2.a);
	System.out.println("a2.AMAX = "+a2.AMAX);

	}
	
}
