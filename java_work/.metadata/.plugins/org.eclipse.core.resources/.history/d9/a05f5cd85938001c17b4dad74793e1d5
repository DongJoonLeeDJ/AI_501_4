class Point{
	int xPos,yPos;
	public Point(int x, int y) {
		xPos = x;
		yPos = y;
	}
	public void showPointInfo() {
		System.out.println("["+xPos+" , "+yPos +"]");
	}
}
class Circle{
	Point p;
	public Circle(int x,int y,int r) {
		this.p = new Point(x, y);
	}
}
public class Ex03{
	public static void main(String[] args) {
		Circle c= new Circle(2,2,4);
		c.showPointInfo();
	}
}
