import aa.AA;

public class Ex02 {

	public static void main(String[] args) {
		AA aarray[] = new AA[3];
		for (int i = 0; i<3; i++) {
			aarray[i] = new AA((i+1)*10,"a"+i);
		}
		
		for (int i = 0; i<3; i++) {
//			System.out.println("aarray["+i+"] = "+aarray[i]);
			aarray[i].doPrint();
		}
		
	}
}
